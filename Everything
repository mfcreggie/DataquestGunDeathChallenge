sexData = [row[5] for row in data]
sex_counts = {}
for sexes in sexData:
    if sexes in sex_counts:
        sex_counts[sexes] +=1
    else:
        sex_counts[sexes] = 0
race_counts = {}
raceData = [row[7] for row in data]
for races in raceData:
    if races in race_counts:
        race_counts[races] +=1
    else:
        race_counts[races] = 0
print (sex_counts)
print (race_counts)
